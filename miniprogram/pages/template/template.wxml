<template name="mokuai">
  <view class="wrap">
    <view class="item">
      <text class="name">工程名称</text>
      <view class="info">{{info.title}}</view>
    </view>
    <view class="item">
        <text class="name">编号</text>
        <view class="info">{{info.id}}</view>
    </view>
    <view class="item">
      <text class="name">日期</text>
      <view class="info">{{info.date}} </view>
    </view>
    <view class="item">
      <text class="name">天气</text>
      <view class="info">{{info.weather}}</view>
    </view>
    <view class="item">
      <text class="name">施工单位</text>
      <view class="info">{{info.shigong}}</view>
    </view>
    
    <view class="item">
      <text class="name">旁站监理的部位或工序</text>
      <view class="info"><view class="box {{!info.reset?'auto':''}}">{{info.process}}:{{info.position}}</view> <image wx:if="{{info.reset}}" src="../../../images/reset.png" bindtap="resetDetail" data-detail="{{info}}"  data-page="/pages/pangzhan/pangzhanFirst/pangzhanFirst"></image> </view>
    </view>
    <view class="item">
      <text class="name">开始时间</text>
      <view class="info">{{info.date}} {{info.startTime}}</view>
    </view>
    <view class="item">
      <text class="name">结束时间</text>
      <view class="info">{{info.date}} {{info.endTime}}</view>
    </view>
    <view class="title">旁站的关键部位、关键工序施工情况</view>
    <view class="item">
      <text class="name">一、作业必备条件检查</text>
      <view class="info">
        <view class="box {{!reset?'auto':''}}">
          <view class="detailItem" wx:for="{{arr}}" wx:key="index">{{item.val}}</view>
        </view>
        <image src="../../../images/reset.png" class="reset" wx:if="{{reset}}" data-detail="{{arr}}" bindtap="resetDetail" data-page="/pages/pangzhan/pangzhanSecond/pangzhanSecond"></image> 
      </view>
    </view>
    <view class="item">
      <text class="name">二、作业过程风险控制措施检查或监理情况</text>
      <view class="info">
        <view class="box {{!reset?'auto':''}}">
          <view class="detailItem2 {{item.checked?'active':''}}" wx:for="{{arr2}}" wx:key="index"> {{index+1}}.作业前是否核实是否停电。<text class="state unChecked" wx:if="{{!item.checked}}"></text><image class="state checked" wx:else src="../../../images/gou.png"></image></view>
        </view>
        <image src="../../../images/reset.png"  wx:if="{{reset}}" bindtap="resetDetail" data-detail="{{arr2}}" data-page="/pages/pangzhan/pangzhanThird/pangzhanThird"></image>
      </view>
    </view>
    <view class="item">
      <text class="name">发现问题及处理</text>
      <view class="info">
        <view class="box {{!reset?'auto':''}}">{{wenti}}</view>
        <image src="../../../images/reset.png"  wx:if="{{reset}}" bindtap="resetDetail" data-detail="{{wenti}}" data-page="/pages/pangzhan/pangzhanfourth/pangzhanfourth"></image>
      </view>
    </view>
    <view class="item">
      <text class="name"> 填写日期</text>
      <view class="info">{{date2}}</view>
    </view>
  </view>
</template>
