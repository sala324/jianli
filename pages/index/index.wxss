.userBox{
  margin: 30rpx auto 50rpx;
  width:690rpx;
  height:160rpx;
  background:rgba(255,255,255,1);
  border:1rpx solid rgba(226,228,230,1);
  box-shadow:0rpx 0rpx 48rpx 3rpx rgba(102,102,102,0.16);
  border-radius:10rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 35rpx;
  box-sizing: border-box;
}
.userBox image{
  width: 88rpx;
  height: 88rpx;
}
.userBox .userName{
  font-size: 38rpx;
  font-weight: bold;
  color: #191F25;
}
.userBox .userId{
  font-weight:500;
  color: #666666;
  font-size: 24rpx;
  line-height: 52rpx;
}
.title{
  font-size: 26rpx;
  color: #999;
  padding:0 0 10rpx 30rpx;
}