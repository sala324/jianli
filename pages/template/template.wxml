<template name="mokuai5">
  <view class="wrap">
    <view class="item">
      <text class="name">工程名称</text>
      <view class="info">{{name}}</view>
    </view>
    <view class="item">
        <text class="name">编号</text>
        <view class="info">0098645</view>
    </view>
    <view class="item">
      <text class="name">检验日期</text>
      <view class="info">2020-05-06</view>
    </view>
    <view class="item">
      <text class="name">检验对象分类</text>
      <view class="info">材料</view>
    </view>
    <view class="item" wx:if="{{type==1}}">
      <text class="name">材料名称</text>
      <view class="info"><view class="box {{!reset?'auto':''}}">{{name}}</view> <image wx:if="{{reset}}" src="../../../images/reset.png" bindtap="resetDetail" data-detail="{{name}}"  data-page="/pages/jingyan/jingyanFirst/jingyanFirst"></image></view>
    </view>
    <view class="item" wx:if="{{type==1}}">
      <text class="name">程材料型号规格</text>
      <view class="info">型号规格1</view>
    </view>
    <view class="item" wx:if="{{type==1}}">
      <text class="name">生产厂家</text>
      <view class="info"><view class="box {{!reset?'auto':''}}">{{actory}}</view> <image wx:if="{{reset}}" src="../../../images/reset.png" bindtap="resetDetail" data-detail="{{actory}}"  data-page="/pages/jingyan/jingyanFirst/jingyanFirst"></image> </view>
    </view>
    <view class="item" wx:if="{{type==1}}">
      <text class="name">使用部位</text>
      <view class="info">使用部位</view>
    </view>
    <view class="item">
      <text class="name"></text>
      <view class="info"><view class="box {{!reset?'auto':''}}"></view> <image wx:if="{{reset}}" src="../../../images/reset.png" bindtap="resetDetail" data-detail="{{arr}}"  data-page="/pages/jingyan/jingyanSecond/jingyanSecond"></image> </view>
    </view>
    <view class="jingyanWrap">
      <view class="jyItem">
        <view class="index">序号</view>
        <view class="name">检验项目</view>
        <view class="size">质量标准</view>
        <view class="size2">质量检验标准</view>
      </view>
      <view class="jyItem" wx:for="{{arr}}" wx:key="index">
        <view class="index">{{index+1}}</view>
        <view class="name">{{item.name}}</view>
        <view class="size" wx:if="{{item.result}}">合格</view>
        <view class="size" wx:else>不合格</view>
        <view class="size2">{{item.standard}}</view>
      </view>

    </view>
    <view class="item">
      <text class="name">检验结论</text>
      <view class="info"><view class="box {{!reset?'auto':''}}">{{arr2[0].val}}</view> <image wx:if="{{reset}}" src="../../../images/reset.png" bindtap="resetDetail" data-detail="{{arr2}}"  data-page="/pages/jingyan/jingyanThird/jingyanThird"></image> </view>
    </view>
    <view class="item">
      <text class="name">检验仪器及编号</text>
      <view class="info">{{arr2[1].val}}</view>
    </view>
  </view>
</template>
