<template name="mokuai">
  <view class="wrap">
    <view class="item">
      <text class="name">工程名称</text>
      <view class="info">{{name}}</view>
    </view>
    <view class="item">
        <text class="name">编号</text>
        <view class="info">{{id}}</view>
    </view>
    <view class="item">
      <text class="name">日期</text>
      <view class="info">{{date}} </view>
    </view>
    <view class="item">
      <text class="name">天气</text>
      <view class="info">{{weather}}</view>
    </view>
    <view class="item">
      <text class="name">施工单位</text>
      <view class="info">{{danwei}}</view>
    </view>
    <view class="item">
      <text class="name">记录标题</text>
      <view class="info"><view class="box {{!reset?'auto':''}}">{{title}}</view> <image wx:if="{{reset}}" src="../../../images/reset.png" bindtap="resetDetail" data-detail="{{title}}"  data-page="/pages/pangzhan/pangzhanFirst/pangzhanFirst"></image> </view>
    </view>
    <view class="item">
      <text class="name">开始时间</text>
      <view class="info">{{startTime}}</view>
    </view>
    <view class="item">
      <text class="name">结束时间</text>
      <view class="info">{{endTime}}</view>
    </view>
    <view class="title">旁站的关键部位、关键工序施工情况</view>
    <view class="item">
      <text class="name">一、作业必备条件检查</text>
      <view class="info">
        <view class="box {{!reset?'auto':''}}">
          <view class="detailItem" wx:for="{{arr}}" wx:key="index"><view wx:for="{{item.items}}" wx:for-item="cell" wx:key="index">{{cell.val}}</view></view>
        </view>
        <image src="../../../images/reset.png" class="reset" wx:if="{{reset}}" data-detail="{{arr}}" bindtap="resetDetail" data-page="/pages/pangzhan/pangzhanSecond/pangzhanSecond"></image> 
      </view>
    </view>
    <view class="item">
      <text class="name">二、作业过程风险控制措施检查或监理情况</text>
      <view class="info">
        <view class="box {{!reset?'auto':''}}">
          <view class="detailItem2 {{item.checked?'active':''}}" wx:for="{{arr2}}" wx:key="index">{{index+1}}.作业前是否核实是否停电。<text wx:if="{{item.checked}}">选中</text></view>
        </view>
        <image src="../../../images/reset.png"  wx:if="{{reset}}" bindtap="resetDetail" data-detail="{{arr2}}" data-page="/pages/pangzhan/pangzhanThird/pangzhanThird"></image>
      </view>
    </view>
    <view class="item">
      <text class="name">发现问题及处理</text>
      <view class="info">
        <view class="box {{!reset?'auto':''}}">{{wenti}}</view>
        <image src="../../../images/reset.png"  wx:if="{{reset}}" bindtap="resetDetail" data-detail="{{wenti}}" data-page="/pages/pangzhan/pangzhanfourth/pangzhanfourth"></image>
      </view>
    </view>
    <view class="item">
      <text class="name"> 填写日期</text>
      <view class="info">{{date2}}</view>
    </view>
  </view>
</template>
